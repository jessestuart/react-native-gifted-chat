{
  "name": "@jesses/react-native-gifted-chat",
  "description": "The most complete chat UI for React Native",
  "version": "0.4.4",
  "author": "Jesse Stuart",
  "bugs": {
    "url": "https://github.com/jessestuart/react-native-gifted-chat/issues"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^1.0.1",
    "moment": "^2.19.0",
    "react-native-communications": "2.2.1",
    "react-native-fit-image": "^1.5.4",
    "react-native-lightbox": "^0.7.0",
    "react-native-markdown-renderer": "^3.2.6",
    "react-native-parsed-text": "^0.0.20",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.12",
    "babel": "6.23.0",
    "babel-jest": "23.0.0-alpha.0",
    "babel-preset-react-native": "4.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-cooperka": "0.2.9",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "3.2.1",
    "husky": "0.15.0-rc.2",
    "jest": "22.1.4",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-native": "0.51.0",
    "react-test-renderer": "16.3.2"
  },
  "homepage": "https://github.com/jessestuart/react-native-gifted-chat#readme",
  "jest": {
    "preset": "react-native",
    "setupFiles": ["./tests/setup.js"],
    "modulePathIgnorePatterns": [
      "<rootDir>/example",
      "<rootDir>/example-expo",
      "<rootDir>/example-slack-message"
    ]
  },
  "keywords": [
    "android",
    "chat",
    "ios",
    "message",
    "messenger",
    "react",
    "react-component",
    "react-native"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-native": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessestuart/react-native-gifted-chat.git"
  },
  "scripts": {
    "appr": "cd example-expo && ./setPRVersion.js && yarn && yarn appr",
    "deploy":
      "cd example-expo && yarn install && exp login --username $EXP_USERNAME --password $EXP_PASSWORD && exp publish --non-interactive",
    "lint": "eslint . --ext .js,.jsx",
    "precommit": "yarn lint",
    "test": "jest --no-watchman",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "types": "index.d.ts"
}
